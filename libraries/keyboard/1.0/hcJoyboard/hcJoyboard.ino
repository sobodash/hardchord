void setup() {
  // Set all the buttons as input, avoiding Serial pins
  for (i = 2; i <= 12 ; i++) pinMode(i, INPUT);
  for (i = 16; i <= 37; i++) pinMode(i, INPUT);
  // Set all the buttons with a pullup resistor, avoiding Serial pins
  for (i = 2; i <= 12 ; i++) digitalWrite(i, HIGH);
  for (i = 16; i <= 37; i++) digitalWrite(i, HIGH);

  Serial.begin(9600);
  Serial3.begin(9600);

  reset_all();

}


void moppyCommand(byte drive, byte high, byte low) {
  unsigned char buf[] = {drives[drive-1], high, low};
  Serial3.write(buf, 3);
  //delay(50);
}

void reset_all() {
  // Park drives
  byte j;
  memset(drives_on, LOW,6);
  for (j=0; j<6; j++)
    moppyCommand(j+1, 0, 0);
//  for (j=0; j<6; j++)
//    drives_on[j] = 0;
  // Clear memory
  memset(keymap_drive, LOW, 36);
  memset(keymap, LOW, 36);
  memset(keymap_bak, LOW, 36);
//  for (j=0; j<36; j++)
//    keymap_drive[j] = 0;
//  for (j=0; j<36; j++) {
//    keymap[j] = HIGH;
//    keymap_bak[j] = HIGH;
//  }
}

void loop() {
  // Build map of keyboard state
  for (i = 2; i <= 37 ; i++) {
    if(i==13 || i==14 || i==15) continue; // this pins are busy with Serial3, and a unstable pin.
    else {
      keymap[k] = digitalRead(i);
      //Serial.print("Polled ");Serial.print(i);Serial.print(" got ");Serial.println(keymap[k]);
      k++;
      //delay(1000);
    }
  }
  
  // Reset key counter
  k=0;
  
  // Check for grind
  if(keymap[32] == LOW) {
    Serial3.write(0x64); // Reset and make loud noise
    //Serial.println("Grind button struck");
  }
  
  // Check for demo
  else if (keymap[31] == LOW) {
    Serial.println("Contra demo button struck");
    reset_all();
    contra();
    reset_all();
  }
  
  // Check for demo
  else if (keymap[30] == LOW) {
    Serial.println("Mario demo button struck");
    reset_all();
    mario();
    reset_all();
  }
  
  // Check for master reset
  else if (keymap[29] == LOW) {
    Serial.println("Reset");
    reset_all();   
  }
  
  // Parse notes
  else {
    // Turn off released keys
    for(i=0; i<29; i++) {
      // Note Release
      if(keymap[i] == HIGH && keymap_bak[i] == LOW) {
        Serial.print("Got key release ");
        Serial.println(i);
        moppyCommand(keymap_drive[i], 0, 0);
        drives_on[keymap_drive[i]-1] = 0;
        keymap_drive[i] = 0;
      }
      
      // Note Engage
      if(keymap[i] == LOW && keymap_bak[i] == HIGH) {
        Serial.print("Got key press ");
        Serial.println(i);
        // Find first disabled drive
        for(int j=0; j<6; j++) {
          if(drives_on[j] == 0) {
            keymap_drive[i] = j+1;
            drives_on[j] = 1;
            break;
          }
        }
        temp_period = periods[i+12*3] / 80;
        moppyCommand(keymap_drive[i], temp_period >> 8, temp_period & 0xff);
        Serial.print("Sent ");Serial.print(temp_period);Serial.print(" to drive ");Serial.println(keymap_drive[i]);
      }
    }
  }

  // Back up last keyboard state
  for(i=0; i<36; i++)
    keymap_bak[i] = keymap[i];
}

